$(document).ready(function(){
   var names = $(".className").toArray();
   names.sort(function(a,b){
       console.log(a.innerHTML + " : " + b.innerHTML);
       return a.innerHTML - b.innerHTML
    })
   for(var i = 0 ; i < names.length; i++){
    console.log(names[i].innerHTML);
   }

});

